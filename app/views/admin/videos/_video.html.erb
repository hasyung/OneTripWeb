<% if !@videos.blank? -%>
	<% @videos.each do |video| -%>
<li class="span3 video_<%= video.id %>">
  
<div id="jp_container_video_<%= video.id %>" class="jp-video jp-video-360p">
			<div class="jp-type-single">
				<div id="jquery_jplayer_video_<%= video.id %>" class="jp-jplayer"></div>
				<div class="jp-gui">
					<div class="jp-video-play">
						<a href="javascript:;" class="jp-video-play-icon" tabindex="1">play</a>
					</div>
					<div class="jp-interface">
						<div class="jp-progress">
							<div class="jp-seek-bar">
								<div class="jp-play-bar"></div>
							</div>
						</div>
						<div class="jp-current-time"></div>
						<div class="jp-duration"></div>
						<div class="jp-title">
							<ul>
								<li><%= video.name %></li>
							</ul>
						</div>
						<div class="jp-controls-holder">
							<ul class="jp-controls">
								<li><a href="javascript:;" class="jp-play" tabindex="1">play</a></li>
								<li><a href="javascript:;" class="jp-pause" tabindex="1">pause</a></li>
							</ul>
						</div>
					</div>
				</div>
				<div class="jp-no-solution">
					<span>Update Required</span>
				</div>
			</div>
		</div>
	<div class="other">
		<%= link_to raw("<i class=\"icon-edit icon-large\"></i>"), 
      edit_admin_place_video_path(video.place_id, video), 
      :title => t("helpers.links.edit"),
      :rel => :tooltip %>
    <%= link_to raw("<i class=\"icon-trash icon-large\"></i>"),
      admin_place_video_path(video.place_id, video),
      :method => :delete,
      :title => t("helpers.links.destroy"),
      :data => { :confirm => t("helpers.messages.confirm") },
      :rel => :tooltip %>
	</div>
</li>
<% end -%>
<% end -%>