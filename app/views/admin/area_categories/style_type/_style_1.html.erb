<fieldset id="map">
	<legend><%= t("helpers.links.map") %></legend>
  <% if @area.images.blank? -%>
  <% else -%>
    <% @area.images.each do |image| %>
    	<%= render :partial => "admin/images/image", :object => image %>
    <% end %>
  <% end %>
	<ul class="control-group unstyled">
    <% if @area.infos.blank? -%>
    <% else -%>
      <% @area.infos.each do |info| %>
        <%= render :partial => "admin/infos/info", :object => info %>
      <% end -%>
    <% end -%>
  </ul>
</fieldset>